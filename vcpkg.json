{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
    "name": "cqsp",
    "version": "0.0.0",
    "dependencies": [
      {
        "name": "openal-soft",
        "default-features": true,
        "platform": "!linux"
      },
      "glfw3",
      "fmt",
      "spdlog",
      "glm",
      "entt",
      {
        "name": "freetype",
        "default-features": false
      },
      "gtest",
      {
        "name": "imgui",
        "default-features": false,
        "features": [
          "freetype",
          "glfw-binding",
          "opengl3-binding"
        ]
      },
      "implot",
      {
        "name": "luajit",
        "platform": "!linux"
      },
      {
        "name": "lua",
        "platform": "!windows"
      }
    ]
}
