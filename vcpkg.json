{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
    "name": "cqsp",
    "version": "0.0.0",
    "builtin-baseline": "efcfaaf60d7ec57a159fc3110403d939bfb69729",
    "dependencies": [
      {
        "name": "openal-soft",
        "default-features": true,
        "platform": "!linux"
      },
      {
        "name": "glfw3",
        "platform": "!linux"
      },
      "fmt",
      "spdlog",
      "glm",
      "entt",
      {
        "name": "freetype",
        "default-features": false
      },
      "gtest",
      {
        "name": "imgui",
        "default-features": false,
        "features": [
          "freetype",
          "glfw-binding",
          "opengl3-binding"
        ]
      },
      "implot",
      {
        "name": "luajit",
        "platform": "!linux"
      },
      {
        "name": "lua",
        "platform": "!windows"
      },
      "date",
      "assimp"
    ],
    "overrides": [
      { "name": "imgui", "version": "1.85" },
      { "name": "entt", "version": "3.10.1" },
      { "name": "fmt", "version": "11.0.2"}
    ]
}
